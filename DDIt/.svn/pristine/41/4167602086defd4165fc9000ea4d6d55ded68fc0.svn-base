/* prettier-ignore */
/*# sourceMappingURL=user.css.map */


@import url('https://fonts.googleapis.com/css2?family=Black+Han+Sans&display=swap');

/* 제목 폰트 */
h1,h2,h3,h4,h5 {
  font-family: "Noto Sans KR", sans-serif;
  font-optical-sizing: auto;
  font-weight: 800;
  font-style: normal;
}

/* 본문 내용 폰트 */
p {
  font-family: "Noto Sans KR", sans-serif;
  font-optical-sizing: auto;
  font-weight: 600;
  font-style: normal;
}


/* 아래 class 추가로도 적용이 가능함. */

/* 두꺼운 글씨용 고딕형 폰트 */
.black-han {
  font-family: "Black Han Sans", sans-serif;
  font-weight: 400;
  font-style: normal;
}

.noto-sans {
  font-family: "Noto Sans KR", sans-serif;
  font-optical-sizing: auto;
  font-weight: 800; /* 400 500 ~ 900 : 100 단위로 폰트 두께 설정 가능 */
  font-style: normal;
}