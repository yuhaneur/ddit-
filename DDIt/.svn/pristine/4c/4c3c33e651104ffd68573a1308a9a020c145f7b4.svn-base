<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.Users.dao.StudentDAO">

<insert id="insertStudent" parameterType="StudentVO">
	INSERT INTO STUDENT (
    USER_ID,
    STUDENT_GENDER,
    CLASS_CODE,
    STUDENT_NAME,
    STUDENT_PHONENO,
    STUDENT_ADDRESS,
    STUDENT_MAIL,
    STUDENT_INTRO
) VALUES (
    #{userId},
    #{studentGender},
    #{classCode},
    #{studentName},
    #{studentPhoneno},
    #{studentAddress},
    #{studentMail},
    #{studentIntro}
)

</insert>

</mapper>