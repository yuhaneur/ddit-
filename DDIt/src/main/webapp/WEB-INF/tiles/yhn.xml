<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd" >
<tiles-definitions>

	<definition name="index" extends="parent">
		<put-attribute name="content" value="/WEB-INF/views/index.jsp" />
	</definition>
	<definition name="school/*" extends="parent">
		<put-attribute name="content" value="/WEB-INF/views/school/{1}.jsp"/>
	</definition>
	<definition name="educate/*" extends="parent">
		<put-attribute name="content" value="/WEB-INF/views/educate/{1}.jsp"/>
	</definition>
	<definition name="subject/*" extends="parent">
		<put-attribute name="content" value="/WEB-INF/views/subject/{1}.jsp"/>
	</definition>

</tiles-definitions>


